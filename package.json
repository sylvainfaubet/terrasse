{
	"name": "terrasse",
	"version": "2.0.0",
	"description": "node version of terrace project",
	"main": "app/index.js",
	"scripts": {
		"start": "webpack-dashboard webpack-dev-server -d --hot --config webpack.config.js --watch",
		"dev": "NODE_ENV=dev webpack",
		"prod": "NODE_ENV=production webpack",
		"serve": "npm run docker:build; npm run docker:run",
		"docker:build": "docker build -t terrasse-nginx .",
		"docker:run": "docker run --name terrasse -d -p 8080:80 terrasse-nginx",
		"docker:stop": "docker stop terrasse; docker rm terrasse"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sylvainfaubet/terrasse.git"
	},
	"author": "faubet.sylvain@gmail.com",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/sylvainfaubet/terrasse/issues"
	},
	"homepage": "https://github.com/sylvainfaubet/terrasse#readme",
	"devDependencies": {
		"autoprefixer": "^8.0.0",
		"babel-core": "^6.26.0",
		"babel-loader": "^7.1.2",
		"babel-preset-env": "^1.6.1",
		"css-loader": "^0.28.9",
		"extract-text-webpack-plugin": "^3.0.2",
		"html-loader": "^0.5.5",
		"html-webpack-plugin": "^2.30.1",
		"node-sass": "^4.7.2",
		"optimize-css-assets-webpack-plugin": "^3.2.0",
		"postcss-loader": "^2.1.0",
		"sass-loader": "^6.0.6",
		"style-loader": "^0.20.2",
		"uglifyjs-webpack-plugin": "^1.2.0",
		"webpack-dashboard": "^1.1.1",
		"webpack-dev-server": "^2.11.1"
	},
	"dependencies": {
		"@uirouter/angularjs": "^1.0.15",
		"ajv": "^6.4.0",
		"angular": "^1.6.9",
		"angular-animate": "^1.6.9",
		"angular-aria": "^1.6.9",
		"angular-material": "^1.1.7",
		"angular-messages": "^1.6.9",
		"webpack": "^3.11.0"
	}
}
